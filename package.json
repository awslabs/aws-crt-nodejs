{
  "name": "aws-crt",
  "version": "0.2.0",
  "description": "NodeJS bindings to the aws-c-* libraries",
  "homepage": "https://github.com/awslabs/aws-crt-nodejs",
  "repository": "github:awslabs/aws-crt-nodejs",
  "contributors": [
    "AWS SDK Common Runtime Team <aws-sdk-common-runtime@amazon.com>"
  ],
  "license": "Apache-2.0",
  "main": "./dist/lib/index.js",
  "browser": "./dist.browser/bundle.js",
  "files": [
    "s2n/",
    "aws-c-common/",
    "aws-c-compression/",
    "aws-c-io/",
    "aws-c-mqtt/",
    "aws-c-http/",
    "aws-c-cal",
    "dist/",
    "dist.browser/",
    "source/",
    "binding.gyp"
  ],
  "gypfile": true,
  "binary": {
    "module_name": "aws-crt-nodejs",
    "module_path": "./dist/napi-v{napi_build_version}-{configuration}",
    "host": "https://aws-crt-nodejs-release.s3.amazonaws.com/",
    "remote_path": "{name}/v{version}/{configuration}",
    "package_name": "{platform}-{arch}-napi-v{napi_build_version}.tar.gz",
    "napi_versions": [
      3
    ]
  },
  "scripts": {
    "build": "tsc && node-pre-gyp rebuild && webpack",
    "test": "node --expose-gc dist/test/test-load.js",
    "install": "tsc && node-pre-gyp install --fallback-to-build && webpack"
  },
  "devDependencies": {
    "@types/crypto-js": "^3.1.43",
    "@types/node": "^10.12.18",
    "@types/ws": "^6.0.1",
    "aws-sdk": "^2.419.0",
    "node-gyp": "^3.8.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "yargs": "^13.2.4"
  },
  "dependencies": {
    "async-mqtt": "^2.3.0",
    "crypto-js": "^3.1.9-1",
    "module-alias": "^2.2.1",
    "mqtt": "^2.3.1",
    "node-pre-gyp": "^0.12.0",
    "websocket-stream": "^5.5.0"
  },
  "_moduleAliases": {
    "@lib": "dist/lib/native"
  }
}
