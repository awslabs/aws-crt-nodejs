{
    "entryPoints": [
        "../lib/native/auth.ts",
        "../lib/native/checksums.ts",
        "../lib/native/crt.ts",
        "../lib/native/crypto.ts",
        "../lib/native/error.ts",
        "../lib/native/http.ts",
        "../lib/native/io.ts",
        "../lib/native/iot.ts",
        "../lib/native/mqtt.ts",
        "../lib/native/mqtt5.ts",
        "../lib/native/binding.d.ts",
